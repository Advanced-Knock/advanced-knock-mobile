{
  "name": "advancedknock",
  "version": "1.0.0",
  "description": "AdvancedKnock - Door-to-door sales app",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "npm run ensure-ngrok && expo start --tunnel",
    "start:lan": "expo start --lan",
    "ensure-ngrok": "npm list @expo/ngrok >/dev/null 2>&1 || npm install --save-dev @expo/ngrok",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "expo": "~54.0.0",
    "expo-build-properties": "~1.0.10",
    "expo-font": "~14.0.10",
    "expo-haptics": "^15.0.8",
    "expo-location": "^19.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "^1.26.20",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-uuid": "^2.0.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/ngrok": "^4.1.3",
    "@tsconfig/react-native": "^3.0.8",
    "@types/react": "~19.1.10",
    "babel-preset-expo": "^54.0.8",
    "typescript": "^5.1.3"
  },
  "private": true
}
